<div class='campos_persona' data-controller='msip--sindocaut'>
  <!-- %= f.input :id, readonly: true %-->
  <%= f.input :id, as: :hidden,
    input_html: {"data-msip--sindocaut-target": "id"}
  %>

  <%= render partial: 'msip/personas/campo_nombres', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_apellidos', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_tdocumento', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_numerodocumento_nobusca', 
    locals: {f: f} %>

  <%= render 'persona_complemento1', locals: {f: f} %>

  <div class="row">
    <div class="col-sm-4">
      <%= render partial: 'msip/personas/campo_mesnac', 
        locals: {f: f} %>
    </div>
    <div class="col-sm-4">
      <%= fvictima.input :edad, as: :integer %>
    </div>
    <div class="col-sm-4">
      <%= fvictima.input :edadactual, as: :integer %>
    </div>
  </div>

  <%= render partial: 'msip/personas/campo_mesnac', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_dianac', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_sexo', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_pais_id', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_departamento_id', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_municipio_id', 
    locals: {f: f} %>

  <%= render partial: 'msip/personas/campo_centropoblado_id', 
    locals: {f: f} %>
</div>
