<%= fields model: @orgsocial do |f| %>
  <%= f.simple_fields_for :orgsocial_persona, 
    child_index: params[:index] do |subf| %>
    <%= turbo_frame_tag "orgsocial_persona_#{subf.index}" do %>
      <%= subf.hidden_field :id, value: params[:id] %>
      <%= subf.hidden_field :_destroy, value: true %>
    <% end %>
  <% end %>
<% end %>
